;; DECOMP BEGINS

;; definition of type wlander-anim-info
(deftype wlander-anim-info (structure)
  ((anim-index        int32  2)
   (anim-index-front  int32)
   (anim-index-back   int32)
   )
  )

;; definition for method 3 of type wlander-anim-info
(defmethod inspect ((this wlander-anim-info))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (format #t "[~8x] ~A~%" this 'wlander-anim-info)
  (format #t "~1Tanim-index[2] @ #x~X~%" (-> this anim-index))
  (format #t "~1Tanim-index-front: ~D~%" (-> this anim-index 0))
  (format #t "~1Tanim-index-back: ~D~%" (-> this anim-index 1))
  (label cfg-4)
  this
  )

;; definition of type wlander-global-info
(deftype wlander-global-info (basic)
  ((prev-yellow-hit          int8)
   (prev-blue-hit            int8)
   (knocked                  int32  2)
   (knocked-land             int32  2)
   (anim-knocked-front       int32)
   (anim-knocked-back        int32)
   (anim-knocked-front-land  int32)
   (anim-knocked-back-land   int32)
   (yellow-hit-anim          int32  2)
   (yellow-land-anim         int32  2)
   (blue-hit-anim            int32  1)
   )
  )

;; definition for method 3 of type wlander-global-info
(defmethod inspect ((this wlander-global-info))
  (when (not this)
    (set! this this)
    (goto cfg-4)
    )
  (format #t "[~8x] ~A~%" this (-> this type))
  (format #t "~1Tprev-yellow-hit: ~D~%" (-> this prev-yellow-hit))
  (format #t "~1Tprev-blue-hit: ~D~%" (-> this prev-blue-hit))
  (format #t "~1Tknocked[2] @ #x~X~%" (-> this knocked))
  (format #t "~1Tknocked-land[2] @ #x~X~%" (-> this knocked-land))
  (format #t "~1Tanim-knocked-front: ~D~%" (-> this knocked 0))
  (format #t "~1Tanim-knocked-back: ~D~%" (-> this knocked 1))
  (format #t "~1Tanim-knocked-front-land: ~D~%" (-> this knocked-land 0))
  (format #t "~1Tanim-knocked-back-land: ~D~%" (-> this knocked-land 1))
  (format #t "~1Tyellow-hit-anim[2] @ #x~X~%" (&-> this anim-knocked-front))
  (format #t "~1Tyellow-land-anim[2] @ #x~X~%" (-> this yellow-hit-anim))
  (format #t "~1Tblue-hit-anim[1] @ #x~X~%" (-> this blue-hit-anim))
  (label cfg-4)
  this
  )

